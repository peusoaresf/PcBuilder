<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uva.pcbuilder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".userinterface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".userinterface.mainbottomview.bottomviewfragments.PartPickerFragment" />
        <activity android:name=".userinterface.mainbottomview.bottomviewfragments.FavoritosFragment" />
        <activity android:name=".userinterface.mainbottomview.bottomviewfragments.PcBuilderFragment"></activity>
    </application>

</manifest>